{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off"
  },
  "trailing-comma": [
    true,
    {
      "singleline": "always",
      "multiline": {
        "objects": "always", // <==================
        "arrays": "always",
        "functions": "never",
        "typeLiterals": "ignore"
      }
    }
  ]
}

